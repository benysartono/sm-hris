<?xml version="1.0" encoding="UTF-8"?>
<xmlquery value="smhris">
		<searchDepartment>Select * from smhris_department</searchDepartment> 
		<searchDepartmentNotEqualTo>Select * from smhris_department where idDepartment != ? </searchDepartmentNotEqualTo> 
		<searchDepartmentP>Select * from smhris_department where name like ?</searchDepartmentP> 
		<departmentDelete>Delete from smhris_department where idDepartment = ?</departmentDelete> 
		<departmentAdd>Insert into smhris_department(idDepartment,name,description,parentDepartment) values(?,?,?,?)</departmentAdd>
		<departmentEdit>Update smhris_department set name=?,description=?,parentDepartment=? where idDepartment=?</departmentEdit>
</xmlquery>

		